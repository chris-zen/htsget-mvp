{
    "htsgetConfig": {
        "props": {
            "port": 8081,
            "host": "http://localhost:8081/"
        },
        "reads": {
            "dataSourceRegistry": {
                "sources": [
                    {
                        "pattern": "^(?P<id>.*)$",
                        "path": "/data/bam/{id}.bam"
                    }
                ]
            }
        }
    }
}